{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string"
        },
        "prefix": {
            "type": "string"
        }
    },
    "functions": [],
    "variables": {
        "AppSVCName": "[concat(parameters('prefix'),'-AppSVC-',parameters('location'))]",
        "AppSVCPlanNAme": "[concat(parameters('prefix'),'-AppSVC-Plan-',parameters('location'))]",
        "VMSSNAme": "[concat(parameters('prefix'),'-APIvmss-',parameters('location'))]",
        "DBServerName": "[concat(parameters('prefix'),'-DBServ-',parameters('location'))]"
    },
    "resources": [
        {
            "type": "Microsoft.Web/sites",
            "apiVersion": "2020-12-01",
            "name": "[variables('AppSVCName')]",
            "location": "[parameters('location')]",
            "kind": "app,linux,container",
            "properties": {
                "enabled": true,
                
        }
    }
    ],
    "outputs": {}
}